{
	"___options": "https://prettier.io/docs/en/options",
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"quoteProps": "consistent",
  "jsxSingleQuote": true,
	"trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": null,
	"htmlWhitespaceSensitivity": "strict",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false
}
